#include<iostream.h>
using namespace std;
void insertion(int arr[],int size)
{
int i,key,j;
for(i=1;i<size;i++)
{
 key=arr[i];
 j=i-1;
     while(j>=0 && arr[j]>key)
     {
	arr[j+1]=arr[j];
	j=j-1;
     }
	arr[j+1}=key;
}
cout<<"the shorted array is:"<<endl;
for(int i=0;i<size;i++)
{
cout<<arr[i]<<""<<endl;
}
}
int main()
{
cout<<"please enter the size of array"<<endl;
int size;
cin>>size;
int array[size];
cout<<"enter array element"<<endl;
for(i=0;i<size;i++)
{
  cin>>array[i];
}
insertion(array,size);
return 0;
}
       		
