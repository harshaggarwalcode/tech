#include<iostream>
#include<string>
using namespace std;
int main()
{
char letter[1000];
cin>>letter;
int n=strlen(letter);
int count=0;
for(int i=0;i<n/2;i++)
{
  if(letter[i]==letter[n-i-1])
  {
    count++;
  }
}
if(count==n/2)
{
   cout<<"string is pallindrom";
}
else
{
cout<<"string is not pallindrom";
}
return 0;
}
